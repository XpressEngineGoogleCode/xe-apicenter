<query id="insertApiKey" action="insert">
    <tables>
        <table name="api_keys" />
    </tables>
    <columns>
        <column name="api_key_srl" var="api_key_srl" filter="number" notnull="notnull" />
		<column name="api_key" var="api_key" notnull="notnull" />
		<column name="member_srl" var="member_srl" notnull="notnull"/>
		<column name="purpose" var="purpose" />
		<column name="regdate" var="regdate" default="curdate()" />
        <column name="last_update" var="last_update" default="curdate()" />
		<column name="extra_vars" var="extra_vars" default=""/>
    </columns>
</query>
