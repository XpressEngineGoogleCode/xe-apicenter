<query id="updateApiGrant" action="update">
    <tables>
        <table name="api_grants" />
    </tables>
    <columns>
		<column name="group_srl" var="group_srl" notnull="notnull" />
    </columns>
    <conditions>
        <condition operation="equal" column="api_item_srl" var="api_item_srl" filter="number" notnull="notnull" pipe="and"/>
		<condition operation="equal" column="api_url_srl" var="api_url_srl" filter="number" notnull="notnull" pipe="and"/>
    </conditions>
</query>
