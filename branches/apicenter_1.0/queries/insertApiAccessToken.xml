<query id="insertApiAccessToken" action="insert">
    <tables>
        <table name="api_access_token" />
    </tables>
    <columns>
		<column name="access_token_srl" var="access_token_srl" notnull="notnull" />
        <column name="access_token" var="access_token" notnull="notnull" />
		<column name="api_url_srl" var="api_url_srl" notnull="notnull" />
		<column name="member_srl" var="member_srl" notnull="notnull" />
		<column name="regdate" default="curdate()" notnull="notnull" />
		<column name="expire" var="expire" />
    </columns>
</query>
