<query id="getApiGrantsByApiItem" action="select">
    <tables>
        <table name="api_grants" alias="api_grants" />
    </tables>
    <columns>
		<column name="*" />
    </columns>
    <conditions>
		 <condition operation="equal" column="api_grants.api_item_srl" var="api_item_srl" notnull="notnull" />
    </conditions>
</query>
