<query id="getApiKeyInfoByKey" action="select">
    <tables>
        <table name="api_keys" alias="api_keys" />
    </tables>
    <columns>
		<column name="*" />
    </columns>
    <conditions>
		 <condition operation="equal" column="api_keys.api_key" var="api_key" notnull="notnull" />
    </conditions>
</query>
