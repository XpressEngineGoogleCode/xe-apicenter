<query id="getApiItemInfo" action="select">
    <tables>
        <table name="api_items" alias="api_items" />
    </tables>
    <columns>
		<column name="*" />
    </columns>
    <conditions>
		 <condition operation="equal" column="api_items.api_item_srl" var="api_item_srl" />
    </conditions>
</query>
